{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "//#format": {},
    "//#format:write": {
      "cache": false
    },
    "//#lint": {},
    "//#lint:write": {
      "cache": false
    },
    "//#lint:write:unsafe": {
      "cache": false
    },
    "//#assist": {},
    "//#assist:write": {
      "cache": false
    },
    "//#check": {},
    "//#check:write": {
      "cache": false
    },
    "//#check:write:unsafe": {
      "cache": false
    }
  }
}
